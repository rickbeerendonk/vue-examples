{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/vue-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/vue-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/vue-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "open": "open",
    "openserver": "node \"./Util - 01 - Custom Server/server.js\" 9002",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,vue}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,vue}'",
    "validate": "npm run eslint && npm run prettier",
    "start": "npm run openserver -- \".\""
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@vue/babel-helper-vue-jsx-merge-props": "1.2.1",
    "@vue/babel-preset-jsx": "1.2.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "7.0.0",
    "core-js": "3.8.3",
    "css-loader": "5.0.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.5.0",
    "html-webpack-plugin": "5.2.0",
    "husky": "4.3.8",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "vue-eslint-parser": "7.4.1",
    "vue-loader": "15.9.6",
    "vue-template-compiler": "2.6.12",
    "webpack": "5.36.2",
    "webpack-cli": "4.4.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "es-module-shims": "0.9.0",
    "http-vue-loader": "1.4.2",
    "serve": "14.0.1",
    "vue-router_3": "npm:vue-router@3.5.4",
    "vue-router_4": "npm:vue-router@4.1.3",
    "vue_2": "npm:vue@2.7.8",
    "vue_3": "npm:vue@3.2.37",
    "vuex_3": "npm:vuex@3.6.2",
    "vuex_4": "npm:vuex@4.0.2"
  }
}
