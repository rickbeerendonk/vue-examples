{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/vue-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/vue-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/vue-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "open": "open",
    "openserver": "node \"./Util - 01 - Custom Server/server.js\" 9002",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,vue}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,vue}'",
    "validate": "npm run eslint && npm run prettier",
    "start": "npm run openserver -- \".\""
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@vue/babel-helper-vue-jsx-merge-props": "1.4.0",
    "@vue/babel-preset-jsx": "1.4.0",
    "@babel/eslint-parser": "7.19.1",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "core-js": "3.27.2",
    "css-loader": "6.7.3",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.9.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.8",
    "prettier": "2.8.3",
    "style-loader": "3.3.1",
    "vue-eslint-parser": "9.1.0",
    "vue-loader": "17.0.1",
    "vue-template-compiler": "2.7.14",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "es-module-shims": "1.6.3",
    "http-vue-loader": "1.4.2",
    "serve": "14.2.0",
    "vue-router": "4.1.6",
    "vue": "3.2.47",
    "vuex": "4.1.0"
  }
}
