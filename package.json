{
  "name": "vue-examples",
  "version": "1.0.0",
  "description": "Examples for [Vue.js](https://vuejs.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/vue-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/vue-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/vue-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.vue --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.vue --cache --fix",
    "open": "open",
    "openserver": "node ./server/server.js",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,vue}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,vue}'",
    "validate": "npm run eslint && npm run prettier",
    "start": "npm run openserver -- \".\""
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue/babel-helper-vue-jsx-merge-props": "1.4.0",
    "@vue/babel-preset-jsx": "1.4.0",
    "@vue/tsconfig": "^0.5.1",
    "babel-loader": "9.2.1",
    "copy-webpack-plugin": "12.0.2",
    "core-js": "3.39.0",
    "css-loader": "7.1.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.9.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.8",
    "npm-run-all2": "^7.0.1",
    "prettier": "3.3.3",
    "style-loader": "3.3.1",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-eslint-parser": "9.4.3",
    "vue-loader": "17.4.2",
    "vue-template-compiler": "2.7.16",
    "vue-tsc": "^2.1.10",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "dependencies": {
    "http-vue-loader": "1.4.2",
    "pinia": "^2.2.6",
    "serve": "14.2.4",
    "vue": "3.5.13",
    "vue-demi": "0.14.10",
    "vue-router": "4.4.5",
    "vuex": "4.1.0"
  }
}
